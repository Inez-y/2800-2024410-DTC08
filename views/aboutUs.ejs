    <style>
        .bouncing-image {
            position: absolute;
            width: 100px;
            /* Adjust as needed */
            height: 100px;
            /* Adjust as needed */
            animation: bounce 5s infinite, rotate 2s infinite linear;
        }

        @keyframes bounce {

            0%,
            100% {
                top: 0;
                left: 0;
            }

            25% {
                top: 0;
                left: calc(100vw - 100px);
            }

            50% {
                top: calc(100vh - 100px);
                left: 0;
            }

            75% {
                top: calc(100vh - 100px);
                left: 0;
                top: calc(100vh - 100px);
                left: calc(100vw - 100px);
            }
        }

        @keyframes rotate {
            0% {
                transform: rotate(0deg);
            }

            100% {
                transform: rotate(360deg);
            }
        }
    </style>
</head>

