    <style>
        .bouncing-image {
            position: absolute;
            width: 100px;
            /* Adjust as needed */
            height: 100px;
            /* Adjust as needed */
            animation: bounce 10s infinite, rotate 2s infinite linear;
        }

        .hidden {
            display: none;
        }

        @keyframes bounce {
            0% {
                top: 10%;
                left: 20%;
            }

            10% {
                top: 40%;
                left: 70%;
            }

            20% {
                top: 80%;
                left: 50%;
            }

            30% {
                top: 30%;
                left: 10%;
            }

            40% {
                top: 60%;
                left: 90%;
            }

            50% {
                top: 20%;
                left: 40%;
            }

            60% {
                top: 70%;
                left: 30%;
            }

            70% {
                top: 50%;
                left: 80%;
            }

            80% {
                top: 10%;
                left: 60%;
            }

            90% {
                top: 40%;
                left: 20%;
            }

            100% {
                top: 10%;
                left: 20%;
            }
        }

        @keyframes rotate {
            0% {
                transform: scale(1) rotate(0deg);
            }

            20% {
                transform: scale(1.5) rotate(160deg);
            }

            40% {
                transform: scale(0.8) rotate(-160deg);
            }

            60% {
                transform: scale(1.2) rotate(180deg);
            }

            80% {
                transform: scale(0.9) rotate(-180deg);
            }

            100% {
                transform: scale(1) rotate(360deg);
            }
        }
    </style>
</head>

