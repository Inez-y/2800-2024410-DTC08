    <script>
        const nameHeaders = document.querySelectorAll('.font-bold');
        const easterEgg = document.getElementById('easterEgg');
        const clickedArray = [0, 0, 0, 0, 0];

        nameHeaders.forEach((div, index) => {
            div.addEventListener('click', () => {
                clickedArray[index - 2] = 1;
                div.classList.add('text-buttongreen');
                checkEasterEggConditions();
            });
        });

        function checkEasterEggConditions() {
            if (clickedArray[0] && clickedArray[1] && clickedArray[2] && clickedArray[3] && clickedArray[4]) {
                console.log('Easter Egg activated!');
                document.querySelectorAll(".hidden").forEach((img, index) => {
                    img.classList.remove('hidden');
                    img.classList.add('bouncing-image');
                    img.style.animationDelay = `${index * 0.5}s`;
                });
            } else {
                console.log('Easter Egg not activated!');
            }
        }
